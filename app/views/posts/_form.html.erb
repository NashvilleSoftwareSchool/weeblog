<%= form_for @post, html: {:multipart => true} do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :body %>
  <%= f.text_field :body %><br \>
  <%= f.label :video_title %>
  <%= f.text_field :video_title %>
  <%= f.label :video_url %>
  <%= f.text_field :video_url %>
  <%= f.label :image %>
  <%= f.file_field :image %>
  <%= f.submit submit_text %>
<%- end %>
