<%= form_for @contact do |f| %>
  <p><%= f.label :name %></p>
  <p><%= f.text_field :name %></p>
 	<p><%= f.label :email %></p>
  <p><%= f.text_field :email %></p>
 	<p><%= f.label :subject %></p>
  <p><%= f.text_field :subject %></p>
  <p><%= f.label :body %></p>
  <p><%= f.text_area :body, :size => "50x10" %></p>
	<%= recaptcha_tags %>
  <p><%= f.submit 'Send Email' %></p>
<%- end %>

